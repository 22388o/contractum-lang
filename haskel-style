type Ticker = str[..8]
type Name = str[..255]

meta Genesis = Ticker, Name, ...

assignment Epoch: decl
assignment Inflation: Maybe Amount
assignment Assets: Amount

transition Transfer :: Assets -> Genesis, Assets -> nonInflationary

nonInflationary :: (Int v) => v -> v -> bool
  [in1 : in2 : _], _ nonInflationary [in1 + in2 : _], _
  [in], _ => nonInflationary in, _
  in, out => in == out
